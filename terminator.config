[global_config]
  handle_size = 0
  title_hide_sizetext = True
  focus = mouse
  title_inactive_fg_color = "#282828"
  enabled_plugins = ,
  title_transmit_bg_color = "#000000"
  inactive_color_offset = 0.36
[keybindings]
  full_screen = None
[profiles]
  [[default]]
    scrollbar_position = hidden
    use_system_font = False
    copy_on_selection = True
    custom_command = env TERM=xterm-256color /usr/local/bin/zsh
    background_image = None
    foreground_color = "#839496"
    use_custom_command = True
    icon_bell = False
    show_titlebar = False
    font = Inconsolata Medium 8
    background_color = "#002b36"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
